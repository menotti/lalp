#define ITEMS (32) 

 void bubble_sort(unsigned int ar[]) {
    unsigned int i,j;
    unsigned int temp;
    //sort!
    for (i=0;i<ITEMS;i++) {
        for (j=0;j<ITEMS-1;j++) {
            if (ar[j] > ar[j+1]) {
                // swap
                temp = ar[j+1];
                ar[j+1] = ar[j];
                ar[j] = temp;
            }
        }
    }
}