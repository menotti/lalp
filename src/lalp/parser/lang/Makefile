JJFILE=ALPParser

# -- files generated by JavaCC --
GENERATED_jj_FILES = \
	ParseException.java \
	Token.java \
	TokenMgrError.java \
	SimpleCharStream.java

# -- files generated by JJTree --
GENERATED_jjt_FILES = \
	Node.java \
	$(JJFILE)TreeConstants.java \
	JJT$(JJFILE)State.java

# -- Run parser generator --
$(JJFILE).java:	$(JJFILE).jj
	rm $(GENERATED_jj_FILES) || true
	javacc $(JJFILE).jj

# -- Run jjtree preprocessor --
$(JJFILE).jj:	$(JJFILE).jjt
	rm $(GENERATED_jjt_FILES) || true
	jjtree $(JJFILE).jjt

